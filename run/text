    .text
    sub     r0, r0, r0  # r0 を 0 にする
    b       _min_caml_start
_min_caml_start:
    lwi     r1, global_var0
    swx     r0, 0(r1)
    fcos    r2, r0
    swx     r2, 1(r1)
    lwx     r60, 1(r1)
    ftoi    r60, r60
    addi    r61, r60, 48
    out     r61
    b       _min_caml_end
_min_caml_end:
    flush
    halt
    halt
    halt    # ここで text セクション終わり
