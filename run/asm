    b _min_caml_start        ##0
dummy:
    addi r1, r1, 100        ##1
_min_caml_start:
    addi r1, r1, 1        ##2
    addi r2, r2, 2        ##3
    cmp  r1, r2        ##4
    bge  dummy        ##5
next:
    addi r1, r1, 1        ##6
    cmp  r1, r2        ##7
    bge jump        ##8
next1:
    addi r3, r3, 10000        ##9
    halt        ##10
jump:
    cmp  r1, r2        ##11
    bge next1        ##12
